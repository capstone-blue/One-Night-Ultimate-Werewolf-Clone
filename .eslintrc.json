{
  "root": true,
  "extends": [
    "fullstack",
    "prettier",
    "prettier/react",
    "plugin:react/recommended"
  ],
  "plugins": ["react"],
  "rules": {
    "semi": 1,
    "no-console": 2,
    "no-alert": 0,
    "complexity": ["error", 13],
    "react/prop-types": 0,
    "react/no-unescaped-entities": 0
  }
}
